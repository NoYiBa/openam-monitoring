{
  "queries": [
    {
      "objectName": "java.lang:type=Memory",
      "resultAlias": "jvm.memory",
      "attributes": [
        {
          "name": "HeapMemoryUsage",
          "keys": [
            "committed",
            "used"
          ]
        },
        {
          "name": "NonHeapMemoryUsage",
          "keys": [
            "committed",
            "used"
          ]
        }
      ]
    },
    {
      "objectName": "java.lang:type=Runtime",
      "resultAlias": "jvm.runtime",
      "attributes": [
        "Uptime"
      ]
    },
    {
      "objectName": "java.lang:type=GarbageCollector,name=*",
      "resultAlias": "jvm.gc.%name%",
      "attributes": [
        "CollectionCount",
        "CollectionTime"
      ]
    },
    {
      "objectName": "java.lang:type=Threading",
      "resultAlias": "jvm.thread",
      "attributes": [
        "ThreadCount"
      ]
    },
    {
      "objectName": "java.lang:type=OperatingSystem",
      "resultAlias": "jvm.os",
      "attributes": [
        "CommittedVirtualMemorySize",
        "FreePhysicalMemorySize",
        "FreeSwapSpaceSize",
        "OpenFileDescriptorCount",
        "ProcessCpuTime",
        "SystemLoadAverage"
      ]
    },
    {
      "obj": "SUN_OPENSSO_SERVER_MIB_SsoServerInstance:name=com.sun.identity.monitoring.SsoServerAuthSvc",
      "attr": [
        "AuthenticationFailureCount",
        "AuthenticationSuccessCount"
      ],
      "resultAlias": "openam.authentication"
    },
    {
      "obj": "SUN_OPENSSO_SERVER_MIB_SsoServerInstance:name=com.sun.identity.monitoring.SsoServerSessSvc",
      "attr": [
        "SessionActiveCount",
        "SessionCreatedCount"
      ],
      "resultAlias": "openam.sessions"
    },
    {
      "obj": "SUN_OPENSSO_SERVER_MIB_SsoServerInstance:name=com.sun.identity.monitoring.SsoServerIdRepoSvc",
      "attr": [
        "IdRepoCacheEntries",
        "IdRepoCacheHits",
        "IdRepoGetRqts",
        "IdRepoSearchCacheHits",
        "IdRepoSearchRqts"
      ],
      "resultAlias": "openam.idrepo"
    }
  ],
  "outputWriters": [
    {
      "@class": "org.jmxtrans.embedded.output.ConsoleWriter",
      "enabled": "${console.enabled:false}"
    },
    {
      "@class": "org.jmxtrans.embedded.output.GraphiteWriter",
      "settings": {
        "host": "${graphite.host:localhost}",
        "port": "${graphite.port:2003}",
        "namePrefix": "${graphite.namePrefix:jmx.#hostname#.}",
        "enabled": "${graphite.enabled:true}"
      }
    }
  ],
   "queryIntervalInSeconds": 30,
   "exportIntervalInSeconds": 60
}
